from AddNumbers import *
 
if __name__ == "__main__":
    add_numbers = AddNumbers()  # Create an instance of AddNumbers
    result1 = add_numbers.sum_of_digits(123)
    result2 = add_numbers.sum_of_digits(111)  # Call the method and store the result
    result3 = add_numbers.sum_of_digits(20055)
   
    print(f"The sum of digits is: {result1}")
    print(f"The sum of digits is: {result2}")
    print(f"The sum of digits is: {result3}")